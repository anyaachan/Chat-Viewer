    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TextFlow prefHeight="451.0" prefWidth="349.0" />
      <Button fx:id="openButton" mnemonicParsing="false" onAction="#openMsgFile" text="Open" />
      <Label fx:id="infoLabel" prefHeight="17.0" prefWidth="430.0" text="Opened Fle" />
   </children>
</VBox>
